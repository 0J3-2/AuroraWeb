{
	"manifest_version": 2,
	"name": "AuroraWeb",
	"version": "0.1.0",
	"content_scripts": [
		{
			"matches": ["*://*.youtube.com/*", "*://youtube.com/*"],
			"css": ["css/youtube.css"],
			"js": ["js/youtube.js"]
		},
		{
			"matches": [
				"*://*.discord.com/*",
				"*://discord.com/*",
				"*://*.discordapp.com/*",
				"*://discordapp.com/*"
			],
			"css": ["css/discord.css"]
		},
		{
			"matches": ["*://cdn.discordapp.com/*", "*://cdn.discord.com/*"],
			"css": ["css/discord.cdn.css"]
		},
		{
			"matches": ["*://support.discord.com/*", "*://*.support.discord.com/*"],
			"css": ["css/support.discord.css"]
		},
		{
			"matches": ["*://web.whatsapp.com/*"],
			"css": ["css/whatsapp.css"]
		},
		{
			"matches": ["*://blox.link/*", "*://*.blox.link/*"],
			"css": ["css/bloxlink.css"],
			"js": ["./js/addPagePath.js"]
		},
		{
			"matches": ["*://roblox.com/*", "*://*.roblox.com/*"],
			"css": ["css/roblox.css"],
			"js": ["./js/addPagePath.js", "./js/roblox.js"]
		},
		{
			"matches": ["*://0j3-2.github.io/AuroraWeb/*"],
			"js": ["js/githubPages.js"]
		}
	]
}
